local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
getgenv().InviteCode = "avmJTQ2Z2Y"
loadstring(game:HttpGet("https://raw.githubusercontent.com/LuaQLeak/Scripts/main/Discord-Auto-Join.lua"))()
local Window = Library.CreateLib("Storm Scripts", "DarkTheme")


-- Credits

local Credits = Window:NewTab("Credits")
local CreditsSection = Credits:NewSection("Credits")

CreditsSection:NewLabel("Discord : Storm Scripts")
CreditsSection:NewLabel("Developer : Black Storm#2573")
CreditsSection:NewLabel("Tester : Vladut#6969")
CreditsSection:NewLabel("Tester : ❄RedDemon❄#8384")



-- Updates

local Updates = Window:NewTab("Updates")
local UpdatesSection = Updates:NewSection("Updates Version 1.0.0V")

UpdatesSection:NewLabel("+Added Anti Afk")
UpdatesSection:NewLabel("+Added Chat Spoof")
UpdatesSection:NewLabel("+Added Walk And Jump Modify")
UpdatesSection:NewLabel("Key Toggle")





-- Essential

local Essential = Window:NewTab("Essential")
local EssentialSection = Essential:NewSection("Caracter")



EssentialSection:NewButton("Anti Afk Script", "Anti Afk", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/GrinLOLZ/Anti-Afk-script-Pokemon-brick-bronze-egg-afk/main/Anti%20afk", true ))()
end)

EssentialSection:NewKeybind("KeybindText", "KeybindInfo", Enum.KeyCode.F, function()
	Library:ToggleUI()
end)


EssentialSection:NewSlider("Walk Speed", "Speed !!", 500, 16, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

EssentialSection:NewSlider("JumpPower", "JUMP JUMP!", 350, 50, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)

EssentialSection:NewButton("Reset WL/JP", "Reset to all defaults", function()
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
end)

EssentialSection:NewLabel("For Fun")

EssentialSection:NewButton("Chat Spoofer", "Lets you chat for other people", function()
    loadstring(game:HttpGet(('https://pastebin.com/raw/djBfk8Li'),true))()
end)





-- PSX
local PSX = Window:NewTab("PSX")
local PSXSection = PSX:NewSection("Auto Farm scripts psx")


PSXSection:NewButton("Project WD (need key)", "Project Wd script for psx", function()
    getgenv().key = "projectWBIsAwesomemrcrapcrappypattt"
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Muhammad6196/Project-WD/main/Main.lua"))()
end)




